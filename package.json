    "@oclif/test": "^3.0.2",
    "@types/chai": "^4.3.10",
    "@types/lodash.pickby": "^4.6.8",
    "@types/lodash.sortby": "^4.7.9",
    "@types/lodash.template": "^4.5.3",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/mocha": "^10.0.4",
    "@types/nock": "^11.1.0",
    "@types/node": "^18",
    "chai": "^4.3.10",
    "commitlint": "^17.8.1",
    "eslint": "^8.53.0",
    "eslint-config-oclif": "^5.0.0",
